<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="624.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AddVolController">
   <children>
      <TextField fx:id="prixvol" layoutX="102.0" layoutY="59.0" />
      <TextField fx:id="nbplacevol" layoutX="102.0" layoutY="116.0" />
      <Label layoutX="29.0" layoutY="64.0" text="prix" />
      <Label layoutX="28.0" layoutY="121.0" text="nbplace" />
      <TableView fx:id="voltab" layoutX="289.0" layoutY="298.0" onMouseClicked="#SetValue" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="prixVoltab" prefWidth="75.0" text="prix" />
          <TableColumn fx:id="nbplaceVoltab" prefWidth="75.0" text="nbplace" />
        </columns>
      </TableView>
      <Button fx:id="btnajouter" layoutX="620.0" layoutY="75.0" mnemonicParsing="false" onAction="#AjouterVol" text="Ajouter" />
      <Button fx:id="btnafficher" layoutX="620.0" layoutY="132.0" mnemonicParsing="false" onAction="#AfficherVol" text="Afficher" />
      <Button fx:id="btnmodifier" layoutX="622.0" layoutY="184.0" mnemonicParsing="false" onAction="#ModifierVol" text="Modifier" />
      <Button fx:id="btnsupprimer" layoutX="622.0" layoutY="237.0" mnemonicParsing="false" onAction="#SupprimerVol" text="Supprimer" />
   </children>
</AnchorPane>
